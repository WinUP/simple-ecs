{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.47.0",
  "pnpmVersion": "6.6.2",
  "pnpmOptions": {
    "pnpmStore": "global",
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=14.15.0 ",
  "projectFolderMaxDepth": 4,
  "repository": {
    "url": "git@github.com:WinUP/SuperSchoolboysRpg.git",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "projects": [
    {
      "packageName": "simple-ecs",
      "projectFolder": "."
    },
    {
      "packageName": "@simple-ecs/core",
      "projectFolder": "packages/core"
    },
    {
      "packageName": "@simple-ecs/runtime-extension",
      "projectFolder": "packages/runtime-extension"
    },
    {
      "packageName": "@simple-ecs/rgui",
      "projectFolder": "packages/rgui"
    }
  ]
}
